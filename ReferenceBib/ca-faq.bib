@TechReport{CocCasBar93,
 label="CocCasBar93",
author =       "Paul Cockshott and George McCaskill and Peter Barrie",
title =        "Use of a high speed cellular automata machine to
               simulate road traffic",
institution =  "Department of Computer Science, University of
               Strathclyde",
year =         "1993",
number =       "HDV-27-93",
abstract =     "We describe a novel method for simulating the
               behaviour of road traffic using reconfigurable logic.
               Roads are modeled as cellular automata whose state
               indicates the presence or absence of cars. The automata
               are then implemented on the SPACE machine, a specialist
               cellular automata simulator, as digital logic circuits.
               Formal proof techniques are used to validate an
               equivalence between the desired behaviour of the
               automata and that of the implementation. Performance
               measures are given indicating a gain of 6 orders of
               magnitude when compared to the use of software
               simulation.",
copy =         "anon ftp at reports-ftp.cs.strath.ac.uk, file
               /research-reports/HDV-27-93.ps.Z",
}

