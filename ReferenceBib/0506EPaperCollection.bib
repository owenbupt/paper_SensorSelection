@PHDTHESIS{OusingsawatEstPathPhD2005,
  author = {Ousingsawat, Jarurat},
  title = {Estimation and path planning for cooperative multi-vehicle systems},
  school = {Cornell University},
  year = {2005},
  abstract = {This work examines the relationship between path planning and cooperative
	estimation for multi-vehicle systems. A unified approach to cooperative
	estimation and path planning for multiple vehicles is presented.
	Cooperative estimation is employed in target tracking, where the
	estimates from different vehicles are combined to reduce target
	uncertainties. Target state information is estimated and integrated
	into a path planning framework. An evolutionary computation based
	planner is used to illustrate vehicle trajectories as they travel
	through an obstacle field. By using information from cooperative
	estimation to identify obstacle locations, vehicles are able to
	avoid obstacles while they travel to goal locations. A cooperative
	estimation approach among vehicles is shown to improve speed and
	routing of the path.
	
	
	Because cooperative estimation significantly depends on vehicle trajectories,
	it is important to take cooperation into account in path planning
	in order to take a full advantage of multiple vehicles. This work
	particularly focuses on cooperative reconnaissance, where the common
	objective of the team is to gather as much information on targets
	as possible. First, a simple case involving two vehicles is studied
	optimally in order to gain a greater understanding of cooperative
	vehicle behavior in reconnaissance. The work makes use of information
	theory to guide the vehicles over paths using a numerical solution
	to the optimal control problem. The effects of triangulation, distance,
	and planning time horizon, are examined. Using the knowledge from
	the simple cases, along with a multiple task assignment algorithm,
	an approximate cooperative reconnaissance methodology is developed.
	Given a set of targets and vehicles, the methodology plans both
	the trajectories and task assignments. In multiple task assignment,
	vehicles are assigned to a set of targets using trajectory primitives
	based on the cost, function. Additional work on clustering and cost
	bounding are used to alleviate the computational load, while maintaining
	performance.},
  owner = {zsong},
  timestamp = {2007.01.18}
}

