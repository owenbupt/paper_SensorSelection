@INPROCEEDINGS{ArandaOptSensorPlacement05,
  author = {Sulema Aranda and Sonia Martinez and Francesco Bullo},
  title = {On optimal sensor placement and motion coordination for target tracking},
  booktitle = {International Conference on Robotics and Automation},
  year = {2005},
  address = {Barcelona, Spain},
  month = apr,
  abstract = {This work studies optimal sensor placement and
	
	motion coordination strategies for mobile sensor networks. For
	
	a target tracking application with range sensors, we investigate
	
	the determinant of the Cramer Rao Lower Bound and compute
	
	it in the 2D and 3D cases, characterizing the global minima in
	
	the 2D case. We propose motion coordination algorithms that
	
	steer the mobile sensor network to an optimal deployment and
	
	that are amenable to a decentralized implementation. Finally,
	
	our numerical simulations illustrate how the proposed algorithms
	
	lead to improved performance of an extended Kalman filter in
	
	a target tracking scenario.},
  owner = {Zhen Song},
  pdf = {\Papers\OED\ArandaOptSensorPlacement05.pdf},
  review = {related work:IEEE TRANSACTIONS ON ROBOTICS AND AUTOMATION, VOL. 20,
