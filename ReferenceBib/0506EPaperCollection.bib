@ARTICLE{ExactTemperatureTracking,
  author = {Marshall Mattingly and Robert B. Roemer and Santosh Devasia},
  title = {Exact Temperature Tracking for Hyperthermia: A Model-Based Approach},
  journal = {IEEE Tran. Control Systems Technology},
  year = {2000},
  volume = {8},
  number = {6},
  month = {dec},
  abstract = {Abstract??n this paper, the inverse dynamics method for generating
	feedforward inputs and reference trajectories to achieve exact tracking
	for general linear time-invariant multiinput??ultioutput (MIMO)
	systems is explicitly derived. The method is applied to the task
	of generating input powers to achieve exact temperature tracking
	(ETT) at multiple locations within a tumor during hyperthermia treatments.
	The inverse dynamics method, which uses the system dynamics to generate
	the feedforward inputs necessary to achieve exact tracking in the
	absence of modeling errors, initial condition errors, and disturbances,
	is applied in conjunction with standard feedback control. This technique
	is compared to the standard tracking controller which uses the desired
	trajectory as the reference trajectory. A number of important issues
	are considered: 1) robustness of the control schemes to parameter
	variation; 2) effect of additive sensor and actuator noise; 3) effect
	of using reduced order model estimators, which is often necessary
	for high-order systems resulting from finite element method models
	commonly used in hyperthermia and large structure control; and 4)
	effect of fast desired trajectories on the accuracy of output tracking.
	The fourth issue is especially important since achieving any desired
	trajectory leads to dose prescribability, a key component to achieving
	increased clinical acceptance of hyperthermia. The inverse dynamics
	method is shown to be significantly more accurate when fast trajectories
	are desired. The results of this simulation study show that model-based
	inverse dynamics control has promise for clinical application. In
	particular, inverse dynamics has the potential to become a useful
	tool for increasing treatment prescribability, increasing treatment
	efficiency, and decreasing patient pain. In addition, the inverse
	dynamics method as derived here can be applied to any other general
	linear MIMO system.},
  keywords = {DPS, Control, estimation, exact tracking, hyperthermia, inverse dynamics,
	modeling, reduced order modeling.},
